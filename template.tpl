___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Load APIs
const addEventCallback = require('addEventCallback');
const getCookieValues = require('getCookieValues');
const json = require('JSON');
const localStorage = require('localStorage');
const log = require('logToConsole');
const setCookie = require('setCookie');

// Set cookie name
const key = '__tid_gtm';

// Generate base variables, and reset storages to empty arrays if not available
let localStorageIDs = localStorage.getItem(key);
localStorageIDs = localStorageIDs ? json.parse(localStorageIDs) : [];
let cookieIDs = getCookieValues(key);
cookieIDs = cookieIDs.length ? json.parse(cookieIDs[0]) : [];
const transactionId = data.transactionId;

// Fail if no transaction ID in the variable
if (!transactionId) {
  log('[Transaction ID Logger / GTM]: No Transaction ID found in the provided variable!');
  return data.gtmOnFailure();
}

// Utility to write the cookie
const writeCookie = () => {
  if (cookieIDs.indexOf(transactionId) === -1) {
    cookieIDs.push(transactionId);
    setCookie(key, json.stringify(cookieIDs), {
      domain: 'auto',
      path: '/',
      'max-age': 2592000,
      secure: true,
      samesite: 'lax'
    });
  }
  return;
};

// Utility to write to localStorage
const writeLocalStorage = () => {
  if (localStorageIDs.indexOf(transactionId) === -1) {
    localStorageIDs.push(transactionId);
    localStorage.setItem(key, json.stringify(localStorageIDs));
  }
  return;
};

// Callback for addEventCallback - write the transaction ID after event has completed.
const writeIds = () => {
  switch (data.storageType) {
    case 'cookie':
      writeCookie();
      break;
    case 'localStorage':
      writeLocalStorage();
      break;
    default:
      writeCookie();
      writeLocalStorage();
  }
};

addEventCallback(writeIds);

data.gtmOnSuccess();


